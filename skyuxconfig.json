{
  "name": "skyux-ag-grid",
  "$schema": "./node_modules/@skyux/config/skyuxconfig-schema.json",
  "mode": "easy",
  "compileMode": "aot",
  "plugins": [
    "@skyux-sdk/builder-plugin-skyux"
  ],
  "testSettings": {
    "e2e": {
      "browserSet": "speedy"
    },
    "unit": {
      "browserSet": "paranoid"
    }
  },
  "pipelineSettings": {
    "vsts": {
      "testSettings": {
        "e2e": {
          "browserSet": false
        },
        "unit": {
          "browserSet": false
        }
      }
    }
  },
  "app": {
    "styles": [
      "src/app/public/styles/ag-grid-styles.scss"
    ]
  },
  "host": {
    "frameOptions": {
      "none": true
    }
  }
}
